{
  "name": "yoink-or-share",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "extract:lang": "ng extract-i18n --output-path src/locales --format json",
    "start": "node ./backend/server.js",
    "start:development": "ng serve",
    "build": "ng build --localize",
    "build:nolocalize": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy-heroku": "git push heroku",
    "ng:lint": "ng lint",
    "lint": "npx prettier --check \"src/**/*.{ts,html,scss}\"",
    "lint:fix": "npx prettier --write \"src/**/*.{ts,html,scss}\""
  },
  "private": true,
